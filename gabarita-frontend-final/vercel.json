{
  "version": 2,
  "name": "gabarita-ai-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://gabarita-ai-backend.onrender.com/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://gabarita-ai-backend.onrender.com",
    "NEXT_PUBLIC_API_BASE_URL": "https://gabarita-ai-backend.onrender.com",
    "NEXT_PUBLIC_BACKEND_URL": "https://gabarita-ai-backend.onrender.com",
    "NEXT_PUBLIC_FRONTEND_URL": "https://gabarita-ai-app.vercel.app"
  }