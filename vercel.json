{
  "version": 2,
  "name": "gabarita-ai-frontend",
  "builds": [
    {
      "src": "gabarita-ai-frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/gabarita-ai-frontend/$1"
    }
  ]
}